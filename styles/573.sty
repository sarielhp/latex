            \typeout{../styles/#1}
            \input{../styles/#1}%
         }{%
            \IfFileExists{../../styles/#1.sty}{
               \input{../../styles/#1}%
            }{%
               \input{#1}%
            }
         }
      }%
      \typeout{}
   }

